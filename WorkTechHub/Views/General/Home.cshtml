<div class="row">
    <!--Notification section start-->
    <div class="col-sm-12 notification " style="font-size:20px; padding-top:10px;">
        <marquee onmouseover="stop()" onmouseout="start()" direction="left">
            @{
                int n = 1;
                foreach (NotificationMaster nm in Model)
                {
                    <i class="fa fa-circle text-danger"style="height:5px;"> </i>
                    <span style="margin-right:10px;">
                        @nm.NMsg
                    @if (n == 1)
                    {
                        <img src="~/content/images/new gif.gif" width="42px" height="28px" />
                    }
                </span>
            }
            }


        </marquee>
    </div>
    <!--Notification section end-->
    
</div>

