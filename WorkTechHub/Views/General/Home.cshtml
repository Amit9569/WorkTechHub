@using WorkTechHub.Models
@model IEnumerable<NotificationMaster>
<script src="~/content/javascript/jquery-3.7.1.min.js"></script>
<link href="~/content/Css/homepage.css" rel="stylesheet" />
<script>
    $(document).ready(function () {
        $("#bttn").click(function () {
            alert("ok");
            var name, email, mobno, msg;
            name = $("#Ename").val();
            email = $("#EEmail").val();
            mobno = $("#Enum").val();
            msg = $("#Emess").val();
            var obj = { Name: name, EmailId: email, MobNo: mobno, Query: msg };
            $.getJSON("/General/SaveEnquiryByAJAX", obj, function (result) {
                if (result == "SUCCESS") {
                    alert("Enquiry saved successfully.");
                    $("#btnclose").trigger("click");
                    $("#Ename").val("");
                    $("#EEmail").val("");
                    $("#Enum").val("");
                    $("#Emess").val("");
                }
                else {
                    alert("Sorry unable to save enquiry");
                }
            });
        });
    });
</script>
