
@{
    ViewBag.Title = "MyProjects";
    Layout = "~/Views/shared/Common_Developer_Layput.cshtml";
}

@using WorkTechHub.Models
@model IEnumerable<RequestMaster>

<div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-10 mt-5">
        <h1 class="text-center pt-2 pb-2 " style="background-color: #AB47BC; color:#FFF; border-radius:10px 10px 0px 0px ;">My Project</h1>
    </div>
    <div class="col-sm-1"></div>
    <div class="col-sm-1"></div>
    <div class="col-sm-10 table-responsive " style="background-color:;">

        <table class="table table-striped table-bordered" style="border: 1px solid #9C27B0;">
            <tr style="background-color:aqua">
                <th>RequestId</th>
                @* <th>Requestedfor</th> *@
                <th class="text-center">Message</th>
                <th>Requested By</th>
                <th>Request Date</th>
            </tr>
            @{

                foreach (RequestMaster rm in Model)

                {

                    <tr>
                        <td  style="background-color: #F3E5F5;">@rm.RequestId</td>

                        @* <td>@rm.ProjectMaster.ProjectId</td> *@
                        <td style="background-color: #F3E5F5;" >@rm.RequestMsg</td>
                        <td  style="background-color: #F3E5F5;">@rm.DeveloperMaster.Email_Id</td>
                        <td style="background-color: #F3E5F5;">@rm.RequestDt</td>

                    </tr>
                }
            }
        </table>
    </div>
    <div class="col-sm-1"></div>
</div>







