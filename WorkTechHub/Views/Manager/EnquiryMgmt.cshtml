
@{
    ViewBag.Title = "EnquiryMgmt";
    Layout = "~/Views/shared/Common_Manager_Layout.cshtml";
}
@using WorkTechHub.Models
@model IEnumerable<EnquiryMaster>
<div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-10 mt-5">
        <h1 class="text-center pt-2 pb-1 " style="background-color: #AB47BC; color:#FFF; border-radius:10px 10px 0px 0px ;">Enquery Management</h1>
    </div>
    <div class="col-sm-1"></div>
    <div class="col-sm-1"></div>
    <div class="col-sm-10 table-responsive">
        
        <table class="table table-striped table-bordered mb-5" style="border: 1px solid #9C27B0;">
            <tr>
                <th>Enquiey Id</th>
                <th>Name</th>
                <th>Email id</th>
                <th>Mobile No.</th>
                <th>Query</th>
                <th>Enquiry Date</th>

            </tr>
            @{

                foreach (EnquiryMaster em in Model)
                {
                    <tr>
                        <td>@em.Enq_Id</td>
                        <td>@em.Name</td>
                        <td>@em.EmailId</td>
                        <td>@em.MobNo</td>
                        <td>@em.Query</td>
                        <td>@em.Enq_DT</td>
                    </tr>
                }


            }
        </table>
    </div>
    <div class="col-sm-1"></div>
</div>
